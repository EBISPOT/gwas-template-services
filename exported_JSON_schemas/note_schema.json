{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "description": "Spreadsheet describing note.",
  "additionalProperties": false,
  "required": [
    "study_tag",
    "note",
    "note_subject",
    "status"
  ],
  "title": "note spreadsheet",
  "name": "note_spreadsheet",
  "type": "object",
  "properties": {
    "study_tag": {
      "description": "A unique free-text label for each genome-wide association study in the publication",
      "type": "string",
      "user_friendly": "Study tag"
    },
    "note": {
      "description": null,
      "type": "string",
      "user_friendly": "Note"
    },
    "note_subject": {
      "description": null,
      "type": "string",
      "user_friendly": "Note subject",
      "ACCEPTED": [
        "Preliminary review",
        "Review/secondary extraction",
        "Post-publishing review",
        "Trait",
        "Initial extraction",
        "Other",
        "Duplication TAG"
      ]
    },
    "status": {
      "description": null,
      "type": "string",
      "user_friendly": "Status",
      "ACCEPTED": [
        "private",
        "public"
      ]
    }
  }
}
